// Change the value based on the value of array
const array = [1,3,4,5,7,9,10,12];
array.forEach((e, index) => {
    switch(e) {
        case 3:
        case 9:
            array[index] = "TEST"
            break;
        case 4:
        case 10:
            array[index] = "DATA"
            break;
        case 12:
            array[index] = "TESTDATA"
            break;
    }
})

console.log(JSON.stringify(array));
